  "dialogues": [
    {
      "speaker": "Mary",
      "japanese": "にほんごをべんきょうしています。",
      "english": "I'm studying Japanese.",
      "audio": "dialogue/001_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "そうですか。あのう、どようびにえいがをみませんか。",
      "english": "I see. Um, won't you watch a movie on Saturday?",
      "audio": "dialogue/002_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "いいですね。なんじにあいますか。",
      "english": "That sounds good. What time shall we meet?",
      "audio": "dialogue/003_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "にじはんにえきのまえであいましょう。",
      "english": "Let's meet in front of the station at 2:30.",
      "audio": "dialogue/004_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "はい、わかりました。たのしみにしています。",
      "english": "OK, got it. I'm looking forward to it.",
      "audio": "dialogue/005_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "メアリーさん、こんにちは。",
      "english": "Hi, Mary.",
      "audio": "dialogue/006_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "あ、たけしさん。こんにちは。",
      "english": "Oh, Takeshi. Hello.",
      "audio": "dialogue/007_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "いま、なにをしていますか。",
      "english": "What are you doing now?",
      "audio": "dialogue/008_takeshi.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "じゃあ、どようびに。",
      "english": "See you Saturday, then.",
      "audio": "dialogue/009_takeshi.mp3"
    }
  ]
