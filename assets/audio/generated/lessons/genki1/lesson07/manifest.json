  "dialogues": [
    {
      "speaker": "Mary",
      "japanese": "背が高くて、きれいな人です。とても優しいです。",
      "english": "She's tall and beautiful. She's very kind.",
      "audio": "dialogue/001_mary.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "はい、高校生です。サッカーが上手です。",
      "english": "Yes, he's a high school student. He's good at soccer.",
      "audio": "dialogue/002_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "メアリーさんの家族は何人ですか。",
      "english": "How many people are in your family, Mary?",
      "audio": "dialogue/003_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "五人です。父と母と姉と弟がいます。",
      "english": "Five people. I have a father, mother, older sister, and younger brother.",
      "audio": "dialogue/004_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "お姉さんは何をしていますか。",
      "english": "What does your older sister do?",
      "audio": "dialogue/005_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "姉は会社員で、東京に住んでいます。",
      "english": "My older sister is an office worker and lives in Tokyo.",
      "audio": "dialogue/006_mary.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "お姉さんはどんな人ですか。",
      "english": "What kind of person is your older sister?",
      "audio": "dialogue/007_takeshi.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "弟さんは学生ですか。",
      "english": "Is your younger brother a student?",
      "audio": "dialogue/008_takeshi.mp3"
    },
    {
      "speaker": "Takeshi",
      "japanese": "いい家族ですね。日本に来たことがありますか。",
      "english": "What a nice family. Have they ever come to Japan?",
      "audio": "dialogue/009_takeshi.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "姉は去年来ました。弟はまだ来たことがありません。",
      "english": "My sister came last year. My brother hasn\\",
      "audio": "dialogue/010_mary.mp3"
    }
  ]
