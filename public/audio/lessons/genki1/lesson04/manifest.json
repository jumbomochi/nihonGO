  "dialogues": [
    {
      "speaker": "Sue",
      "japanese": "デートはどうでしたか。",
      "english": "How was the date?",
      "audio": "dialogue/001_sue.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "とても楽しかったです。",
      "english": "It was very fun.",
      "audio": "dialogue/002_mary.mp3"
    },
    {
      "speaker": "Sue",
      "japanese": "どこに行きましたか。",
      "english": "Where did you go?",
      "audio": "dialogue/003_sue.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "映画を見て、レストランに行きました。",
      "english": "We watched a movie and went to a restaurant.",
      "audio": "dialogue/004_mary.mp3"
    },
    {
      "speaker": "Sue",
      "japanese": "レストランはどこにありましたか。",
      "english": "Where was the restaurant?",
      "audio": "dialogue/005_sue.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "駅の近くにありました。",
      "english": "It was near the station.",
      "audio": "dialogue/006_mary.mp3"
    },
    {
      "speaker": "Sue",
      "japanese": "何を食べましたか。",
      "english": "What did you eat?",
      "audio": "dialogue/007_sue.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "たけしさんはラーメンを食べました。私はカレーを食べました。",
      "english": "Takeshi ate ramen. I ate curry.",
      "audio": "dialogue/008_mary.mp3"
    },
    {
      "speaker": "Sue",
      "japanese": "いいですね。また会いますか。",
      "english": "That's nice. Will you meet again?",
      "audio": "dialogue/009_sue.mp3"
    },
    {
      "speaker": "Mary",
      "japanese": "はい、来週の土曜日に会います。",
      "english": "Yes, we'll meet next Saturday.",
      "audio": "dialogue/010_mary.mp3"
    }
  ]
