  "dialogues": [
    {
      "id": "l01-d01",
      "title": "At the Cafeteria",
      "lines": [
        {
          "speaker": "Mary",
          "japanese": "すみません。いま なんじですか。",
          "english": "Excuse me. What time is it now?",
          "audio": "dialogue/d01_001_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "じゅうにじはんです。",
          "english": "It's twelve thirty.",
          "audio": "dialogue/d01_002_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "ありがとうございます。",
          "english": "Thank you very much.",
          "audio": "dialogue/d01_003_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "あのう、りゅうがくせいですか。",
          "english": "Um, are you an international student?",
          "audio": "dialogue/d01_004_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "はい、アリゾナだいがくの がくせいです。なまえは メアリー・ハートです。",
          "english": "Yes, I'm a student at the University of Arizona. My name is Mary Hart.",
          "audio": "dialogue/d01_005_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "そうですか。ぼくは たけしです。よねんせいです。せんこうは れきしです。メアリーさんの せんこうは？",
          "english": "I see. I'm Takeshi. I'm a fourth-year student. My major is history. What's your major, Mary?",
          "audio": "dialogue/d01_006_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "にほんごです。いま にねんせいです。",
          "english": "Japanese. I'm a sophomore now.",
          "audio": "dialogue/d01_007_mary.mp3"
        }
      ]
    },
    {
      "id": "l01-d02",
      "title": "At a Welcome Party",
      "lines": [
        {
          "speaker": "Ken",
          "japanese": "ゆきさん、こちらは リサさんです。カナダじんです。",
          "english": "Yuki, this is Lisa. She is Canadian.",
          "audio": "dialogue/d02_001_ken.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "はじめまして。リサです。よろしくおねがいします。",
          "english": "Nice to meet you. I am Lisa. Please treat me well.",
          "audio": "dialogue/d02_002_lisa.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "ゆきです。こちらこそ、よろしく。",
          "english": "I'm Yuki. Nice to meet you too.",
          "audio": "dialogue/d02_003_yuki.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "ゆきさんは だいがくせいですか。",
          "english": "Are you a university student, Yuki?",
          "audio": "dialogue/d02_004_lisa.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "はい、さんねんせいです。せんこうは けいざいがくです。",
          "english": "Yes, I'm a third-year student. My major is economics.",
          "audio": "dialogue/d02_005_yuki.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "そうですか。わたしは にほんごの がくせいです。",
          "english": "I see. I'm a Japanese language student.",
          "audio": "dialogue/d02_006_lisa.mp3"
        }
      ]
    },
    {
      "id": "l01-d03",
      "title": "Meeting Your Neighbor",
      "lines": [
        {
          "speaker": "Kenji",
          "japanese": "あ、こんにちは。となりの へやですか。",
          "english": "Oh, hello. Are you in the next room?",
          "audio": "dialogue/d03_001_kenji.mp3"
        },
        {
          "speaker": "Tom",
          "japanese": "はい、そうです。トムです。オーストラリアから きました。",
          "english": "Yes, that's right. I'm Tom. I came from Australia.",
          "audio": "dialogue/d03_002_tom.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "おれは けんじ。にほんじん。よろしく。",
          "english": "I'm Kenji. Japanese. Nice to meet you.",
          "audio": "dialogue/d03_003_kenji.mp3"
        },
        {
          "speaker": "Tom",
          "japanese": "よろしく。けんじさんは なんねんせい？",
          "english": "Nice to meet you. What year are you, Kenji?",
          "audio": "dialogue/d03_004_tom.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "いちねんせい。トムさんは？",
          "english": "First year. How about you, Tom?",
          "audio": "dialogue/d03_005_kenji.mp3"
        },
        {
          "speaker": "Tom",
          "japanese": "りゅうがくせいだから... えっと、にねんせいかな。",
          "english": "Since I'm an exchange student... um, maybe second year?",
          "audio": "dialogue/d03_006_tom.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "そっか。まあ、よろしくね！",
          "english": "I see. Well, nice to meet you!",
          "audio": "dialogue/d03_007_kenji.mp3"
        }
      ]
    },
    {
      "id": "l01-d04",
      "title": "In the Classroom",
      "lines": [
        {
          "speaker": "Professor",
          "japanese": "はい、みなさん、おはようございます。やまだです。",
          "english": "Alright everyone, good morning. I am Yamada.",
          "audio": "dialogue/d04_001_professor.mp3"
        },
        {
          "speaker": "Professor",
          "japanese": "しゅっせきを とります。キムさん？",
          "english": "I'll take attendance. Kim?",
          "audio": "dialogue/d04_002_professor.mp3"
        },
        {
          "speaker": "Kim",
          "japanese": "はい。",
          "english": "Here.",
          "audio": "dialogue/d04_003_kim.mp3"
        },
        {
          "speaker": "Professor",
          "japanese": "キムさんは どこから ですか。",
          "english": "Kim, where are you from?",
          "audio": "dialogue/d04_004_professor.mp3"
        },
        {
          "speaker": "Kim",
          "japanese": "かんこくから です。かんこくじんです。",
          "english": "I'm from Korea. I am Korean.",
          "audio": "dialogue/d04_005_kim.mp3"
        },
        {
          "speaker": "Professor",
          "japanese": "そうですか。ロバートさんは？",
          "english": "I see. How about Robert?",
          "audio": "dialogue/d04_006_professor.mp3"
        },
        {
          "speaker": "Robert",
          "japanese": "アメリカじんです。カリフォルニアから きました。",
          "english": "I'm American. I came from California.",
          "audio": "dialogue/d04_007_robert.mp3"
        },
        {
          "speaker": "Professor",
          "japanese": "みなさん、どうぞ よろしく。",
          "english": "Everyone, nice to meet you all.",
          "audio": "dialogue/d04_008_professor.mp3"
        }
      ]
    },
    {
      "id": "l01-d05",
      "title": "Business Introduction",
      "lines": [
        {
          "speaker": "Tanaka",
          "japanese": "しつれいします。ABCかいしゃの たなかと もうします。",
          "english": "Excuse me. I am Tanaka from ABC Company.",
          "audio": "dialogue/d05_001_tanaka.mp3"
        },
        {
          "speaker": "Chen",
          "japanese": "はじめまして。XYZの チェンと もうします。ちゅうごくじんです。",
          "english": "Nice to meet you. I am Chen from XYZ. I am Chinese.",
          "audio": "dialogue/d05_002_chen.mp3"
        },
        {
          "speaker": "Tanaka",
          "japanese": "どうぞ よろしく おねがいいたします。これ、めいしです。",
          "english": "Very pleased to meet you. Here is my business card.",
          "audio": "dialogue/d05_003_tanaka.mp3"
        },
        {
          "speaker": "Chen",
          "japanese": "ありがとうございます。わたくしの めいしも どうぞ。",
          "english": "Thank you. Please take my card as well.",
          "audio": "dialogue/d05_004_chen.mp3"
        },
        {
          "speaker": "Tanaka",
          "japanese": "チェンさんは えいぎょうぶですか。",
          "english": "Are you in the sales department, Ms. Chen?",
          "audio": "dialogue/d05_005_tanaka.mp3"
        },
        {
