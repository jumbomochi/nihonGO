  "dialogues": [
    {
      "speaker": "Yuki",
      "japanese": "ロバートさん、毎日何をしていますか。",
      "english": "Robert, what do you do every day?",
      "audio": "dialogue/001_yuki.mp3"
    },
    {
      "speaker": "Robert",
      "japanese": "朝六時に起きて、シャワーを浴びます。",
      "english": "I wake up at 6 a.m. and take a shower.",
      "audio": "dialogue/002_robert.mp3"
    },
    {
      "speaker": "Yuki",
      "japanese": "朝ご飯は何を食べますか。",
      "english": "What do you eat for breakfast?",
      "audio": "dialogue/003_yuki.mp3"
    },
    {
      "speaker": "Robert",
      "japanese": "パンを食べて、コーヒーを飲みます。",
      "english": "I eat bread and drink coffee.",
      "audio": "dialogue/004_robert.mp3"
    },
    {
      "speaker": "Yuki",
      "japanese": "今、何をしていますか。",
      "english": "What are you doing now?",
      "audio": "dialogue/005_yuki.mp3"
    },
    {
      "speaker": "Robert",
      "japanese": "日本語を勉強しています。",
      "english": "I'm studying Japanese.",
      "audio": "dialogue/006_robert.mp3"
    },
    {
      "speaker": "Yuki",
      "japanese": "宿題をしてから、何をしますか。",
      "english": "What will you do after doing homework?",
      "audio": "dialogue/007_yuki.mp3"
    },
    {
      "speaker": "Robert",
      "japanese": "友達に会って、一緒に映画を見ます。",
      "english": "I'll meet a friend and watch a movie together.",
      "audio": "dialogue/008_robert.mp3"
    },
    {
      "speaker": "Yuki",
      "japanese": "その友達はどんな人ですか。",
      "english": "What is that friend like?",
      "audio": "dialogue/009_yuki.mp3"
    },
    {
      "speaker": "Robert",
      "japanese": "背が高くて、髪が短い人です。とても優しいです。",
      "english": "He's tall with short hair. He's very kind.",
      "audio": "dialogue/010_robert.mp3"
    }
  ]
