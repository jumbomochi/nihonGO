  "dialogues": [
    {
      "id": "l03-d01",
      "title": "Movie Invitation",
      "lines": [
        {
          "speaker": "Takeshi",
          "japanese": "メアリーさん、こんにちは。",
          "english": "Hi, Mary.",
          "audio": "dialogue/d01_001_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "あ、たけしさん。こんにちは。",
          "english": "Oh, Takeshi. Hello.",
          "audio": "dialogue/d01_002_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "いま、なにをしていますか。",
          "english": "What are you doing now?",
          "audio": "dialogue/d01_003_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "にほんごをべんきょうしています。",
          "english": "I'm studying Japanese.",
          "audio": "dialogue/d01_004_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "そうですか。あのう、どようびにえいがをみませんか。",
          "english": "I see. Um, won't you watch a movie on Saturday?",
          "audio": "dialogue/d01_005_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "いいですね。なんじにあいますか。",
          "english": "That sounds good. What time shall we meet?",
          "audio": "dialogue/d01_006_mary.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "にじはんにえきのまえであいましょう。",
          "english": "Let's meet in front of the station at 2:30.",
          "audio": "dialogue/d01_007_takeshi.mp3"
        },
        {
          "speaker": "Mary",
          "japanese": "はい、わかりました。たのしみにしています。",
          "english": "OK, got it. I'm looking forward to it.",
          "audio": "dialogue/d01_008_mary.mp3"
        }
      ]
    },
    {
      "id": "l03-d02",
      "title": "Dinner Plans",
      "lines": [
        {
          "speaker": "Yuki",
          "japanese": "すずきさん、きょうのよるは おひまですか。",
          "english": "Mr. Suzuki, are you free this evening?",
          "audio": "dialogue/d02_001_yuki.mp3"
        },
        {
          "speaker": "Suzuki",
          "japanese": "はい、とくによていは ありません。",
          "english": "Yes, I don't have any particular plans.",
          "audio": "dialogue/d02_002_suzuki.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "じゃあ、いっしょに ゆうごはんを たべませんか。",
          "english": "Then, won't you have dinner together with me?",
          "audio": "dialogue/d02_003_yuki.mp3"
        },
        {
          "speaker": "Suzuki",
          "japanese": "いいですね。どこで たべますか。",
          "english": "That sounds good. Where shall we eat?",
          "audio": "dialogue/d02_004_suzuki.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "えきのちかくに あたらしい イタリアりょうりの レストランが ありますよ。",
          "english": "There's a new Italian restaurant near the station.",
          "audio": "dialogue/d02_005_yuki.mp3"
        },
        {
          "speaker": "Suzuki",
          "japanese": "いいですね。なんじに いきますか。",
          "english": "Sounds good. What time shall we go?",
          "audio": "dialogue/d02_006_suzuki.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "ろくじはんは どうですか。",
          "english": "How about 6:30?",
          "audio": "dialogue/d02_007_yuki.mp3"
        },
        {
          "speaker": "Suzuki",
          "japanese": "いいですよ。ろくじはんに しましょう。",
          "english": "Sure. Let's make it 6:30.",
          "audio": "dialogue/d02_008_suzuki.mp3"
        }
      ]
    },
    {
      "id": "l03-d03",
      "title": "Declining an Invitation",
      "lines": [
        {
          "speaker": "Ken",
          "japanese": "リサさん、きんようびの よるは ひまですか。",
          "english": "Lisa, are you free on Friday evening?",
          "audio": "dialogue/d03_001_ken.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "きんようびですか。なにか ありますか。",
          "english": "Friday? Is there something?",
          "audio": "dialogue/d03_002_lisa.mp3"
        },
        {
          "speaker": "Ken",
          "japanese": "はい、いっしょに カラオケに いきませんか。",
          "english": "Yes, won't you go to karaoke together?",
          "audio": "dialogue/d03_003_ken.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "すみません、きんようびは ちょっと...",
          "english": "Sorry, Friday is a little... (I can't)",
          "audio": "dialogue/d03_004_lisa.mp3"
        },
        {
          "speaker": "Ken",
          "japanese": "ああ、そうですか。ざんねんですね。",
          "english": "Oh, I see. That's too bad.",
          "audio": "dialogue/d03_005_ken.mp3"
        },
        {
          "speaker": "Lisa",
          "japanese": "どようびは どうですか。どようびなら だいじょうぶです。",
          "english": "How about Saturday? Saturday would be fine.",
          "audio": "dialogue/d03_006_lisa.mp3"
        },
        {
          "speaker": "Ken",
          "japanese": "どようびですか。いいですよ。じゃあ、どようびに しましょう。",
          "english": "Saturday? Sure. Then let's make it Saturday.",
          "audio": "dialogue/d03_007_ken.mp3"
        }
      ]
    },
    {
      "id": "l03-d04",
      "title": "Weekend Plans",
      "lines": [
        {
          "speaker": "Robert",
          "japanese": "けんじさん、しゅうまつは なにを しますか。",
          "english": "Kenji, what are you doing this weekend?",
          "audio": "dialogue/d04_001_robert.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "どようびの あさは テニスを します。",
          "english": "I'll play tennis on Saturday morning.",
          "audio": "dialogue/d04_002_kenji.mp3"
        },
        {
          "speaker": "Robert",
          "japanese": "いいですね。ごごは なにを しますか。",
          "english": "That's nice. What will you do in the afternoon?",
          "audio": "dialogue/d04_003_robert.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "ごごは ともだちと まちに いきます。かいものを します。",
          "english": "In the afternoon, I'll go to town with a friend. We'll go shopping.",
          "audio": "dialogue/d04_004_kenji.mp3"
        },
        {
          "speaker": "Robert",
          "japanese": "にちようびは？",
          "english": "How about Sunday?",
          "audio": "dialogue/d04_005_robert.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "にちようびは うちで やすみます。ロバートさんは？",
          "english": "On Sunday, I'll rest at home. How about you, Robert?",
          "audio": "dialogue/d04_006_kenji.mp3"
        },
        {
          "speaker": "Robert",
          "japanese": "どようびは びょういんに いきます。にちようびは としょかんで べんきょうします。",
          "english": "Saturday I'll go to the hospital. Sunday I'll study at the library.",
          "audio": "dialogue/d04_007_robert.mp3"
        },
        {
          "speaker": "Kenji",
          "japanese": "そうですか。おだいじに。",
          "english": "I see. Take care.",
          "audio": "dialogue/d04_008_kenji.mp3"
        }
      ]
    },
    {
      "id": "l03-d05",
      "title": "Confirming Plans",
      "lines": [
        {
          "speaker": "Yuki",
          "japanese": "もしもし、たけしさん。ゆきです。",
          "english": "Hello, Takeshi. This is Yuki.",
          "audio": "dialogue/d05_001_yuki.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "ああ、ゆきさん。こんばんは。",
          "english": "Oh, Yuki. Good evening.",
          "audio": "dialogue/d05_002_takeshi.mp3"
        },
        {
          "speaker": "Yuki",
          "japanese": "あしたの よていを かくにんしたいんですが...",
          "english": "I'd like to confirm tomorrow's plans...",
          "audio": "dialogue/d05_003_yuki.mp3"
        },
        {
          "speaker": "Takeshi",
          "japanese": "はい、じゅうじに しぶやえきで あいますね。",
          "english": "Yes, we're meeting at Shibuya Station at 10, right?",
