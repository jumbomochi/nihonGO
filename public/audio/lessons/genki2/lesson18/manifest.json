  "dialogues": [
    {
      "speaker": "Friend",
      "japanese": "ジョン、さいきんいそがしそうだね。",
      "english": "John, you seem busy lately.",
      "audio": "dialogue/001_friend.mp3"
    },
    {
      "speaker": "John",
      "japanese": "うん、コンビニでアルバイトをはじめたんだ。",
      "english": "Yeah, I started a part-time job at a convenience store.",
      "audio": "dialogue/002_john.mp3"
    },
    {
      "speaker": "Friend",
      "japanese": "へえ、どう？たいへん？",
      "english": "Oh, really? How is it? Is it tough?",
      "audio": "dialogue/003_friend.mp3"
    },
    {
      "speaker": "John",
      "japanese": "はじめはたいへんだったけど、だんだんなれてきたよ。",
      "english": "It was tough at first, but I've gradually gotten used to it.",
      "audio": "dialogue/004_john.mp3"
    },
    {
      "speaker": "Friend",
      "japanese": "よかったね。しごとは何をするの？",
      "english": "That's good. What do you do at work?",
      "audio": "dialogue/005_friend.mp3"
    },
    {
      "speaker": "John",
      "japanese": "レジをしながら、しなものをならべたりするんだ。",
      "english": "I arrange products while working the register.",
      "audio": "dialogue/006_john.mp3"
    }
  ]
